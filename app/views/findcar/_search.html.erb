<%= form_for(@resource_schedule) do |f| %>
  <% if @resource_schedule.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@resource_schedule.errors.count, "error") %> prohibited this resource_schedule from being saved:</h2>

      <ul>
      <% @resource_schedule.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  From <input id="resource_schedule_startplace" name="resource_schedule[startplace]" size="30" style="width:100px" type="text">
