<%= form_for @preference, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :user_id_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :user_id_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :copassangerGender, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :copassangerGender, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :acNonac, :class => 'control-label' %>
    <div class="controls">
      <%= f.check_box :acNonac, :class => 'check_box' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :carModel, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :carModel, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :maxCapacity, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :maxCapacity, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :from, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :from, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :to, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :to, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :startTime, :class => 'control-label' %>
    <div class="controls">
      <%= f.time_select :startTime, :class => 'time_select' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                preferences_path, :class => 'btn' %>
  </div>
<% end %>
