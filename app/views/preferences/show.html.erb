<%- model_class = Preference -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:user_id_id) %>:</strong></dt>
  <dd><%= @preference.user_id_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:copassangerGender) %>:</strong></dt>
  <dd><%= @preference.copassangerGender %></dd>
  <dt><strong><%= model_class.human_attribute_name(:acNonac) %>:</strong></dt>
  <dd><%= @preference.acNonac %></dd>
  <dt><strong><%= model_class.human_attribute_name(:carModel) %>:</strong></dt>
  <dd><%= @preference.carModel %></dd>
  <dt><strong><%= model_class.human_attribute_name(:maxCapacity) %>:</strong></dt>
  <dd><%= @preference.maxCapacity %></dd>
  <dt><strong><%= model_class.human_attribute_name(:from) %>:</strong></dt>
  <dd><%= @preference.from %></dd>
  <dt><strong><%= model_class.human_attribute_name(:to) %>:</strong></dt>
  <dd><%= @preference.to %></dd>
  <dt><strong><%= model_class.human_attribute_name(:startTime) %>:</strong></dt>
  <dd><%= @preference.startTime %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              preferences_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_preference_path(@preference), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              preference_path(@preference),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
