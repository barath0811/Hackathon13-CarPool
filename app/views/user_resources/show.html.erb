<%- model_class = UserResource -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @user_resource.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:acNonac) %>:</strong></dt>
  <dd><%= @user_resource.acNonac %></dd>
  <dt><strong><%= model_class.human_attribute_name(:carModel) %>:</strong></dt>
  <dd><%= @user_resource.carModel %></dd>
  <dt><strong><%= model_class.human_attribute_name(:maxCapacity) %>:</strong></dt>
  <dd><%= @user_resource.maxCapacity %></dd>
  <dt><strong><%= model_class.human_attribute_name(:imageUrl) %>:</strong></dt>
  <dd><%= @user_resource.imageUrl %></dd>
  <dt><strong><%= model_class.human_attribute_name(:carNumber) %>:</strong></dt>
  <dd><%= @user_resource.carNumber %></dd>
  <dt><strong><%= model_class.human_attribute_name(:isOwned) %>:</strong></dt>
  <dd><%= @user_resource.isOwned %></dd>
  <dt><strong><%= model_class.human_attribute_name(:driverName) %>:</strong></dt>
  <dd><%= @user_resource.driverName %></dd>
  <dt><strong><%= model_class.human_attribute_name(:isSelfDrive) %>:</strong></dt>
  <dd><%= @user_resource.isSelfDrive %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              user_resources_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_user_resource_path(@user_resource), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              user_resource_path(@user_resource),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
