<%= form_for @user_resource, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :user_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :user_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :acNonac, :class => 'control-label' %>
    <div class="controls">
      <%= f.check_box :acNonac, :class => 'check_box' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :carModel, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :carModel, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :maxCapacity, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :maxCapacity, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :imageUrl, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :imageUrl, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :carNumber, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :carNumber, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :isOwned, :class => 'control-label' %>
    <div class="controls">
      <%= f.check_box :isOwned, :class => 'check_box' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :driverName, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :driverName, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :isSelfDrive, :class => 'control-label' %>
    <div class="controls">
      <%= f.check_box :isSelfDrive, :class => 'check_box' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                user_resources_path, :class => 'btn' %>
  </div>
<% end %>
